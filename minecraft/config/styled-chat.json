{
  "CONFIG_VERSION_DONT_TOUCH_THIS": 2,
  "_comment": "Before changing anything, see https://github.com/Patbox/StyledChat#configuration",
  "defaultStyle": {
    "displayName": "${vanillaDisplayName}",
    "chat": "<gray><cmd:'/tell %player:name_visual% '><hover:'<gray>Click to message.'>${player}</hover></cmd>: ${message}",
    "join": "<gray>[<c:#4bc957>+</c>] ${player}",
    "joinRenamed": "<gray>[<c:#4bc957>+</c>] ${old_name} → ${player}",
    "joinFirstTime": "<gray>[<c:#4bc957>+</c>] ${player} <rainbow>First join!</rainbow>",
    "left": "<gray>[<c:#b52b2b>-</c>] ${player}",
    "death": "${default_message}",
    "advancementTask": "<><lang:chat.type.advancement.task:'${player}':'${advancement}'>",
    "advancementChallenge": "<lang:chat.type.advancement.challenge:'${player}':'${advancement}'>",
    "advancementGoal": "<lang:chat.type.advancement.goal:'${player}':'${advancement}'>",
    "teamChatSent": "<lang:'chat.type.team.sent':'<hover\\:\\'<lang\\:chat.type.team.hover>\\'><suggest_command\\:\\'/teammsg \\'>${team}':'${displayName}':'${message}'>",
    "teamChatReceived": "<lang:'chat.type.team.text':'<hover\\:\\'<lang\\:chat.type.team.hover>\\'><suggest_command\\:\\'/teammsg \\'>${team}':'${displayName}':'${message}'>",
    "privateMessageSent": "<dark_gray>[<c:#2bb555>PM → ${receiver}</c>] <gray>${message}",
    "privateMessageReceived": "<dark_gray>[<c:#33ff70>PM ← ${sender}</c>] <gray>${message}",
    "sayCommand": "[${player}] ${message}",
    "meCommand": "<lang:'chat.type.emote':'${player}':'${message}'>"
  },
  "permissionStyles": [
    {
      "permission": "admin",
      "opLevel": -1,
      "style": {
        "displayName": "<color:#b53735>${vanillaDisplayName}</color>",
        "chat": "<dark_gray>[<gradient:#ff5c5c:#c92c2c>Admin</gradient>]</dark_gray> <gray><cmd:'/tell %player:name_visual% '><hover:'<gray>Click to message.'>%player:name%</hover></cmd>: ${message}"
      }
    }
  ],
  "petDeathMessage": "${default_message}",
  "emoticons": {
    "potion": "🧪",
    "item": "[%player:equipment_slot/mainhand%]",
    "trident": "🔱",
    "rod": "🎣",
    "shrug": "¯\\_(ツ)_/¯",
    "bow": "🏹",
    "bell": "🔔",
    "heart": "❤",
    "bucket": "🪣",
    "sword": "🗡",
    "shears": "✂",
    "pos": "%player:pos_x% %player:pos_y% %player:pos_z%",
    "fire": "🔥",
    "table": "(╯°□°）╯︵ ┻━┻"
  },
  "permissionEmoticons": [],
  "legacyChatFormatting": false,
  "parseLinksInChat": true,
  "enableMarkdown": true,
  "formattingInPrivateMessages": true,
  "formattingInTeamMessages": true,
  "linkStyle": "<underline><c:#0000EE>${link}",
  "spoilerStyle": "<gray>${spoiler}",
  "spoilerSymbol": "▌",
  "defaultEnabledFormatting": {
    "dark_red": false,
    "color": false,
    "underline": true,
    "yellow": false,
    "italic": true,
    "dark_blue": false,
    "dark_purple": false,
    "gold": false,
    "red": false,
    "aqua": false,
    "hover": false,
    "gray": false,
    "light_purple": false,
    "white": false,
    "dark_gray": false,
    "spoiler": true,
    "strikethrough": true,
    "lang": false,
    "obfuscated": false,
    "dark_grey": false,
    "key": false,
    "change_page": false,
    "st": true,
    "b": true,
    "green": false,
    "c": false,
    "dark_green": false,
    "gradient": false,
    "black": false,
    "em": false,
    "i": true,
    "bold": true,
    "gr": false,
    "grey": false,
    "orange": false,
    "rb": false,
    "rainbow": false,
    "obf": false,
    "colour": false,
    "blue": false,
    "dark_aqua": false,
    "underlined": false,
    "reset": false,
    "page": false,
    "font": false
  }
}